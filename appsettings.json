{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=SharedHostingDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Storage": {
    "Type": "Local",
    "Local": {
      "BasePath": "wwwroot/uploads"
    },
    "Ftp": {
      "Host": "ftp.example.com",
      "Username": "user",
      "Password": "pass"
    },
    "S3": {
      "BucketName": "mybucket",
      "Region": "us-east-1"
    }
  }
}
